<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Define a custom sequence for your model -->
    <record id="seq_hospital_pateint" model="ir.sequence">
        <field name="name">Hospital Patient</field>
        <field name="code">hospital.pateint</field>
        <field name="prefix">HP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="appointment_sequence_data" model="ir.sequence">
        <field name="name">Hospital.appointment</field>
        <field name="code">hospital.appointment</field>
        <field name="prefix">OP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>