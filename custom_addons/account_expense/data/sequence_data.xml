<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Define the sequence for classroom model -->
           <record id="sequence_account_move_sequence" model="ir.sequence">
        <field name="name">Expense Sequence</field>
        <field name="code">account.move.sequence</field>
        <field name="prefix">EXP/%(year)s/%(month)s/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False"/>
    </record>

              <record id="sequence_expense_sequence" model="ir.sequence">
            <field name="name">sale Sequence</field>
            <field name="code">account.move.expense.sequence</field>
            <field name="prefix">EXP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>