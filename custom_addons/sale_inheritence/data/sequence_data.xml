<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Define the sequence for classroom model -->
        <record id="sequence_sale_sequence" model="ir.sequence">
            <field name="name">sale Sequence</field>
            <field name="code">sale.sequence</field>
            <field name="prefix">ES</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="sequence_expense_sequence" model="ir.sequence">
            <field name="name">Expense Sequence</field>
            <field name="code">expense.sequence</field>
            <field name="prefix">EXP/</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>